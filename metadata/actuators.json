	"translation": {
		"items": {
			"outputs_v1": {
				"list": {
					"key": "label",
					"items": {
						"subgroups": {
							"list": {
								"items": {
									"parameters": {
										"list": {
											"key": "name",
											"translate": [ "label" ]
										}
									},
									"per-channel-parameters": {
										"list": {
											"key": "name",
											"translate": [ "label" ]
										}
									},
									"channels": {
										"list": {
											"key": "param-index",
											"translate": [ "label" ]
										}
									}
								}
							}
						}
					}
				}
			},
			"functions_v1": {
				"items": {
					"*": {
						"translate": [ "label" ],
						"items": {
							"note": {
								"translate": [ "text" ]
							}
						}
					}
				}
			},
			"mixer_v1": {
				"items": {
					"actuator-types": {
						"items": {
							"*": {
								"items": {
									"per-item-parameters": {
										"list": {
											"key": "name",
											"translate": [ "label" ]
										}
									}
								}
							}
						}
					},
					"config": {
						"list": {
							"key": "option",
							"translate": [ "title" ],
							"items": {
								"actuators": {
									"list": {
										"translate": [ "group-label", "item-label-prefix" ],
										"items": {
											"parameters": {
												"list": {
													"key": "name",
													"translate": [ "label" ]
												}
											},
											"per-item-parameters": {
												"list": {
													"key": "name",
													"translate": [ "label" ]
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
